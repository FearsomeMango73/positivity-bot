  <patternset id="excluded.from.module.positivity-bot_main">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.positivity-bot_main">
    <patternset refid="excluded.from.module.positivity-bot_main"/>
  </patternset>
  
  <path id="positivity-bot_main.module.sourcepath">
    <dirset dir="${module.positivity-bot_main.basedir}/../../src/main">
      <include name="java"/>
      <include name="resources"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.positivity-bot_main" depends="compile.module.positivity-bot_main.production,compile.module.positivity-bot_main.tests" description="Compile module positivity-bot_main"/>
  
  <target name="compile.module.positivity-bot_main.production" depends="register.custom.compilers" description="Compile module positivity-bot_main; production classes">
    <mkdir dir="${positivity-bot_main.output.dir}"/>
    <javac2 destdir="${positivity-bot_main.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.positivity-bot_main}/javac">
      <compilerarg line="${compiler.args.positivity-bot_main}"/>
      <bootclasspath refid="positivity-bot_main.module.bootclasspath"/>
      <classpath refid="positivity-bot_main.module.production.classpath"/>
      <src refid="positivity-bot_main.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.positivity-bot_main"/>
    </javac2>
    
    <copy todir="${positivity-bot_main.output.dir}">
      <fileset dir="${module.positivity-bot_main.basedir}/../../src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.positivity-bot_main.basedir}/../../src/main/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.positivity-bot_main.tests" depends="register.custom.compilers,compile.module.positivity-bot_main.production" description="compile module positivity-bot_main; test classes" unless="skip.tests"/>
  
  <target name="clean.module.positivity-bot_main" description="cleanup module">
    <delete dir="${positivity-bot_main.output.dir}"/>
    <delete dir="${positivity-bot_main.testoutput.dir}"/>
  </target>
